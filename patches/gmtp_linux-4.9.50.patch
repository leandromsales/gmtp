+        pr_info("ISS: %u, GSS: %u, N: %u\n", gmtp_sk(sk)->iss,
+                gmtp_sk(sk)->gss,
+                (gmtp_sk(sk)->gss - gmtp_sk(sk)->iss));
+    }
+}
