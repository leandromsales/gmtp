+static int gmtp_v4_client_rcv_reporter_ack(struct sk_buff *skb,
+        struct gmtp_client *c)
+{
+    const struct iphdr *iph = ip_hdr(skb);
+    const struct gmtp_hdr *gh = gmtp_hdr(skb);
+
+    gmtp_pr_func();
+
+    if(c == NULL) {
+        gmtp_pr_error("Client is NULL!");
+        return 1;
