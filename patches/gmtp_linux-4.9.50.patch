+int gmtp_retransmit_skb(struct sock *sk);
+struct sk_buff *gmtp_ctl_make_elect_response(struct sock *sk,
+        struct sk_buff *rcv_skb);
+struct sk_buff *gmtp_ctl_make_ack(struct sock *sk, struct sk_buff *rcv_skb);
