+/* return largest index i such that fval <= lookup[i][small] */
+static inline u32 mcc_binsearch(u32 fval, u8 small)
+{
+	u32 try, low = 0, high = MCC_CALC_X_ARRSIZE - 1;
+
+	while (low < high) {
+		try = (low + high) / 2;
+		if (fval <= mcc_calc_x_lookup[try][small])
+			high = try;
+		else
+			low  = try + 1;
+	}
+	return high;
+}
