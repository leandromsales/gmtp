+#include <linux/init.h>
+#include <linux/module.h>
+#include "mcc_proto.h"
+
+int __init mcc_lib_init(void)
+{
+	int rc = mcc_li_init();
+
+	mcc_pr_debug("Starting GMTP-MCC");
+
+	rc = mcc_rx_packet_history_init();
+	if (rc)
