+static void gmtp_do_redirect(struct sk_buff *skb, struct sock *sk)
+{
+    struct dst_entry *dst = __sk_dst_check(sk, 0);
+
+    if(dst)
+        dst->ops->redirect(dst, sk, skb);
+}
